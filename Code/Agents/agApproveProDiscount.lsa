%REM
	Function doApprove
	Description: Comments for Function
%END REM
Function doApprove(curDoc As NotesDocument,curStage As String)
	On Error GoTo errHandler
	isRecalculated = curDoc.Getitemvalue(FIELD_RECALCULATION)(0)
	If(curStage="1") Then
		Call submitDocument(curDoc)
	ElseIf(curStage="2") Then
		Call asmApproval(curDoc)
	ElseIf(curStage="2.1") Then
		Call rsmApproval(curDoc)
		
	ElseIf(curStage="3") Then
		Call mktgUpload(curDoc)
	ElseIf(curStage="4") Then
		Call nsmApproval(curDoc)
	ElseIf(curStage="4.1") Then
		Call nsmRework(curDoc)
	ElseIf(curStage="5") Then
		Call vpApproval(curDoc)
	ElseIf(curStage="5.1") Then
		Call vpRework(curDoc)
	ElseIf(curStage="6") Then
		Call mdApproval(curDoc)
	ElseIf(curStage="6.1") Then
		Call mdRework(curDoc)
	ElseIf(curStage="7") Then
		Call completeOrder(curDoc)
	End If	
	Exit Function
errHandler:
	MsgBox "Error in doApprove function: "+Error+ ": at line number: "+CStr(Erl)
	Exit Function
End Function

