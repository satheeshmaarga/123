Sub Initialize()
	Dim curDoc As NotesDocument
	Dim curStatus As String
	Dim curStage As String
	Dim rtnAns As Boolean
	Dim session As New NotesSession
	Dim statusView As NotesView  
	Dim viewEntryCol As NotesViewEntryCollection
	Dim viewEntry As NotesViewEntry
	Dim mailDoc As NotesDocument
	Dim btnSource As String
	Dim flSize As Boolean
	Dim saveOpt As String
	Dim stage As String
	
	On Error GoTo ErrHandler
	Set session=New NotesSession
	Set curDb=session.currentDatabase
	docReaders=""
	docAuthors=""
	
	Set statusView=curDb.Getview("vwByStatus")
	
	If statusView Is Nothing Then
		Print "The vwByStatus view is null"
		Exit Sub
	End If
	
	'Get the all the entry from mail inbox
	Set viewEntryCol=statusView.Allentries
	
	If viewEntryCol.Count=0  Then
		Print "The vwByStatus view  is empty"
		Exit Sub
	End If
	Set viewEntry=ViewEntryCol.Getfirstentry()
	While Not ViewEntry Is Nothing
		
		'Check whether the view entry is document or not
		If Not viewEntry.Isdocument Then
			GoTo whileEnd
		End If
		'Get the corresponding maildocument from mailview entry
		Set curDoc=viewEntry.Document
		
		If curDoc Is Nothing Then
			Print "The vwByStatus view entry not contains a document"
			GoTo whileEnd
		End If
		
		Call curDoc.Replaceitemvalue("SaveOption","1")
		'prevApprover=curDoc.curUserName(0)
		
		curStatus=curDoc.Getitemvalue(FIELD_STATUS)(0)
		
		btnSource=curDoc.Getitemvalue(FIELD_BTNSOURCE)(0)					
		asm=curDoc.casm(0)	
	
		If curDoc.ReadyForSchedule(0)="yes" Then
			If(btnSource="0") Then	
				Call Approve(curDoc)
			ElseIf (btnSource="1") Then
				Call Reject(curDoc)
			End If	
		End If
whileEnd:
		curDoc.ReadyForSchedule=""
		Call curDoc.Computewithform(False,False)	
			Call curDoc.Save(true,False)
		Set ViewEntry=ViewEntryCol.Getnextentry(ViewEntry)			
	Wend		
	Exit Sub
ErrHandler:
	MsgBox "Error occured in agent 'agApproveProDiscount' at sub initialize: "+Error+ ": at line number: "+CStr(Erl)+"Error number: "+CStr(Err)
	Exit Sub
End Sub

