	'Getting SE Name from document.. That SE Name act As key for getting area configuration document.
	seName=curDoc.Getitemvalue(FIELD_SALES_EXE)(0)
	'Get the ASM name from document.
	asmName=curDoc.Getitemvalue(FIELD_ASM)(0)
	key=seName
	Dim keyVal As Variant	
	'Get the Sales executive name
	salesExe=curDoc.getItemValue(FIELD_SALES_EXE)(0)
	'get the area configuration document
	Set areaConfigDoc=getLookupDoc(key)
	If areaConfigDoc Is Nothing Then
		MsgBox "Error in taking handle of area configuration document....!"
		Exit Function 
	End If
	
	'Getting RSM,NSM,VP,MD,and LE name from area configuration document.
	rsmName=getRSMName(areaConfigDoc)
	mktgName=getMKTGName(areaConfigDoc)
	nsmName=getNSMName(areaConfigDoc)
	vpName=getVPName(areaConfigDoc)
	mdName=getMDName(areaConfigDoc)
	leName=getLEName(areaConfigDoc)
	
	'Get the current approver
	currentstage=curDoc.Getitemvalue(FIELD_CURRENT_STAGE)(0)
	'Set stage as 9 indicate rejected document
	Call curDoc.Replaceitemvalue(FIELD_CURRENT_STAGE,"9")
	'Set the next approver is empty
	Call curDoc.Replaceitemvalue(FIELD_NEXT_APPROVER, "")
	'Current stage-2 then send mail to SE
	stage1=curDoc.Getitemvalue(FIELD_CUR_STAGE)(0)
	'currentstage=2 then the request rejected by ASM
	If(currentstage="2") Then
		Call curDoc.Replaceitemvalue(FIELD_SAVE_OPT, "-1")
		Call curDoc.Replaceitemvalue(FIELD_STATUS_MSG,"Rejected-ASM")
		Call updateEditHistory(curDoc, "Rejected by ASM",asmname)
		'Set ASM is Author
		Call updateReaderString(asmName,curDoc)
		docAuthors=seName		
		Call RejectsendMail(curDoc,seName,"",asmname)
		
	'currentstage=2.1 then the request rejected by RSM
	ElseIf(currentstage="2.1") Then
		sender(0)=salesExe
		sender(1)=asmName
		sender(2)=seName
		Call curDoc.Replaceitemvalue(FIELD_STATUS_MSG,"Rejected-RSM")
		Call updateEditHistory(curDoc, "Rejected by RSM",rsmName)
		'Set the reader as ASM,RSM
		Call updateReaderString(asmName,curDoc)
		Call updateReaderString(rsmName,curDoc)
		'Set the author as SE
		docAuthors=seName
		Call RejectsendMail(curDoc,seName,"",rsmName)
		
		Call RejectsendMail(curDoc,"",asmname,rsmName)
		Exit Function
	
		'currentstage=3 then the request rejected by MKTG
	ElseIf(currentstage="3") Then
		sender(0)=salesExe
		sender(1)=asmName
		sender(2)=seName
		sender(3)=mktgName
		Call curDoc.Replaceitemvalue(FIELD_STATUS_MSG,"Rejected-MKTG")
		Call updateEditHistory(curDoc, "Rejected-MKTG",mktgName)
		'Set the reader as ASM,RSM
		Call updateReaderString(asmName,curDoc)
		Call updateReaderString(mktgName,curDoc)
		'Set the author as SE
		docAuthors=mktgName
		Call RejectsendMail(curDoc,seName,"",nsmName)	
		Exit Function
	
	'currentstage=4 then the request rejected by NSM
	ElseIf(currentstage="4" Or currentStage="4.1" Or CurrentStage="6.1" Or CurrentStage="7.1") Then
		Call curDoc.Replaceitemvalue(FIELD_STATUS_MSG,"Rejected-NSM")
		Call updateEditHistory(curDoc, "Rejected by NSM",nsmName)
		sender(0)=salesExe
		sender(1)=asmName
		sender(2)=seName
		sender(3)=mktgName
		rsmName=getRSMName(areaConfigDoc)
		nsmName=getNSMName(areaConfigDoc)
		If Not(Trim(rsmName)=Trim(nsmName)) Then 
			sender(4)=rsmName
			'Set the reader as ASM,NSM
			Call updateReaderString(asmName,curDoc)
			Call updateReaderString(nsmName,curDoc)
			'Set the author as SE
			docAuthors=seName			
		End If
		Call RejectsendMail(curDoc,seName,"",nsmName)
		
		Call RejectSendMail(curDoc,"",asmname,nsmname)
		Call RejectSendMail(curDoc,"",rsmname,nsmname)
		Call RejectSendMail(curDoc,"",mktgname,nsmname)
		Exit Function
	
	'currentstage=5 then the request rejected by VP
%REM	
	ElseIf(currentstage="5") Then
		Call curDoc.Replaceitemvalue(FIELD_STATUS_MSG,"Rejected-VP")
		Call updateEditHistory(curDoc, "Rejected by VP",vpName)
		sender(0)=salesExe
		sender(1)=asmName
		sender(2)=mktgName
		rsmName=getRSMName(areaConfigDoc)
		nsmName=getNSMName(areaConfigDoc)
		If Not(Trim(rsmName)=Trim(nsmName)) Then 
			sender(3)=rsmName
			sender(4)=nsmName
			'Set the reader as ASM,RSM,NSM,VP
			Call updateReaderString(asmName,curDoc)
			Call updateReaderString(rsmName,curDoc)
			Call updateReaderString(nsmName,curDoc)
			Call updateReaderString(vpName,curDoc)
			'Set the author as SE
			docAuthors=seName				
		Else
			sender(3)=nsmName
			'Set the reader as ASM,NSM,VP
			Call updateReaderString(asmName,curDoc)
			Call updateReaderString(nsmName,curDoc)
			Call updateReaderString(vpName,curDoc)
			'Set the author as SE
			docAuthors=seName	
		End If
		Call RejectsendMail(curDoc,seName,"",vpName)
		Exit Function
	'currentstage=6 then the request rejected by MD
	ElseIf(currentstage="6") Then
		Call curDoc.Replaceitemvalue(FIELD_STATUS_MSG,"Rejected-MD")
		Call updateEditHistory(curDoc, "Rejected by MD",mdName)
		sender(0)=salesExe
		sender(1)=curDoc.Getitemvalue(FIELD_ASM)(0)
		sender(2)=mktgName
		rsmName=getRSMName(areaConfigDoc)
		nsmName=getNSMName(areaConfigDoc)
		If Not(Trim(rsmName)=Trim(nsmName)) Then 
			sender(3)=rsmName
			sender(4)=nsmName
			sender(5)=vpName
			'Set the reader as ASM,RSM,NSM,VP,MD
			Call updateReaderString(asmName,curDoc)
			Call updateReaderString(nsmName,curDoc)
			Call updateReaderString(rsmName,curDoc)
			Call updateReaderString(vpName,curDoc)
			Call updateReaderString(mdName,curDoc)
			'Set the author as SE
			docAuthors=seName			
		Else
			sender(3)=nsmName
			sender(4)=vpName
			'Set the reader as ASM,NSM,VP,MD
			Call updateReaderString(asmName,curDoc)
			Call updateReaderString(nsmName,curDoc)
			Call updateReaderString(vpName,curDoc)
			Call updateReaderString(mdName,curDoc)
			'Set the author as SE
			docAuthors=seName	
		End If		
		
	
	Call updateReader("", curDoc)
	Call updateReader(docAuthors, curDoc)
	'Set the author as ASM
	Call updateAuthor(docAuthors, curDoc)
	Call curDoc.Computewithform(True,False)
	Call RejectsendMail(curDoc,seName,"",mdName)
	%END REM
	End If
	Exit Function
ErrHandler:
	Print "Error in Reject function: "+Error+ ": at line number: "+CStr(Erl)
	Exit Function
End Function
