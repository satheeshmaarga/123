%REM
	Function calculteFileSize
	Description: Comments for Function
%END REM
Function calculteFileSize(doc As NotesDocument)

	Dim embedObj As NotesEmbeddedObject
	Dim files As Variant
	Dim fileCount As Integer
	Dim jpgpdf As Boolean
	Dim xltype As Boolean
	Dim sizeCheck As Boolean
	
	'Error Handler Function 
	On Error GoTo errHandler
	
	If doc.Hasembedded=True Then
		files=Evaluate(|@AttachmentNames|,doc)
		For fileCount=0 To UBound(files)			
			Set embedObj=doc.Getattachment(files(fileCount))
			jpgpdf=FileTypeJpgPdf(files(fileCount))
			xltype=FileTypeXl(files(fileCount))
			If  jpgpdf Then
				If embedObj.FileSize &lt;=716800  Then					
					calculteFileSize=True					
				Else					
					calculteFileSize=False
					Call embedObj.Remove()
				End If	
			ElseIf  xltype=True Then
				If embedObj.FileSize &lt;=307200  Then					
					calculteFileSize=True
				Else 
					calculteFileSize=False
					Call embedObj.Remove()
				End If	
			Else
				calculteFileSize=False
				Call embedObj.Remove()
			End If
		Next
	Else
		calculteFileSize=True
	End If
	Exit Function
errHandler:
	Print "Error in function calculateFileSize in an agent agentProjectRequest "+Error+" at line number: "+CStr(Erl)+"Error code: "+CStr(Err)
	Exit Function 
End Function

