	Set curDb=session.currentDatabase
	Dim configPNView As NotesView
	Dim configPNDoc As NotesDocument
	Dim docYear As String
	Dim docPN As String
	Dim btnSource As String
	Dim perConfigView As NotesView
	Dim perConfigDoc As NotesDocument
	Dim rowcount As Integer
	Dim columncount As Integer
