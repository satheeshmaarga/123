Function leStage(curDoc As NotesDocument,sendername As String)
	On Error GoTo errHandler
	
	Call curDoc.Replaceitemvalue(FIELD_CURRENT_STAGE,"7")
	Call curDoc.Replaceitemvalue(FIELD_STATUS_MSG,"Awaiting Order Entry")
	Call updateEditHistory(curDoc,"Awaiting Order Entry",leName)
	Call updateReaderString(seName,curDoc)
	Call updateReaderString(getCanonicalName(rlmName),curDoc)
	'Set the authors as LE
	docAuthors=leName
	Call updateAuthor(leName, curDoc)
	Call curDoc.Replaceitemvalue(FIELD_NEXT_APPROVER, leName)
	Call sendMail(curDoc,leName,"",sendername)
	Exit Function
errHandler:
	MsgBox "Error in leStage function: "+Error+ ": at line number: "+CStr(Erl)
	Exit Function
End Function

