%REM
	Function updateReader
	Description: Comments for Function
%END REM
Function updateReaderString(readerName As String,curDoc As NotesDocument)
	
	
	On Error GoTo ErrHandler
	
	docReaders=curDoc.getItemValue("bufferItem")(0)
	
	If Trim(docReaders) &lt;&gt;"" Then
		If Trim(readerName)&lt;&gt;"" Then	docReaders=docReaders+";"+readerName
	Else
		If Trim(readerName)&lt;&gt;"" Then	docReaders=readerName
	End If
	
	Call curDoc.replaceItemValue("bufferItem",docReaders)
	
	Exit Function
errHandler:
	Print "Error in function updateReaderString in an agent agentProjectRequest "+Error+" at line number: "+CStr(Erl)+"Error code: "+CStr(Err)
	Exit Function 
End Function

