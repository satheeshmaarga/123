%REM
	Function updateReader
	Description: Comments for Function
%END REM
Function updateReader(readerName As String, requestDoc As NotesDocument)
	
	Dim session As New NotesSession
	Dim readers As Variant
	Dim readerNotesName As NotesName
	Dim docReaderItem As NotesItem
	Dim buffer As String
	Dim readerArray As Variant
	Dim iniSize As Integer
	
	On Error GoTo ErrHandler
	
	readerArray=requestDoc.Getitemvalue("DocumentReader")
	iniSize=UBound(readerArray)
	
	buffer=""
	readerArray=FullTrim(readerArray)
	ForAll reader In readerArray
		buffer=buffer+getCanonicalName(reader)+";"
	End ForAll
	
	Set readerNotesName=New NotesName(readerName)
	
	ReDim Preserve readerArray(iniSize+1)
	buffer=buffer+readerNotesName.Canonical
	
	readerArray=ArrayUnique(Split(buffer,";"),0)
	readerArray=FullTrim(readerArray)
	Call requestDoc.Replaceitemvalue("DocumentReader", Split(Join(readerArray,";"),";"))
	Call requestDoc.Computewithform(True,False)
	
	Set docReaderItem=requestDoc.Getfirstitem("DocumentReader")
	
	docReaderItem.IsReaders=True
	Exit Function
	
errHandler:
	Print "Error in function updateReader in an agent agentProjectRequest "+Error+" at line number: "+CStr(Erl)+"Error code: "+CStr(Err)
	Exit Function 

End Function

