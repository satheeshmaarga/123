%REM
	Function FileTypeXl
	Description: Comments for Function
%END REM
Function FileTypeXl(fileName As String) As Boolean
	'Error Handler Function 
	On Error GoTo errHandler
	
	If (LCase(StrRightBack(fileName,"."))="xlsx")Or(LCase(StrRightBack(fileName,"."))="xls")Or (LCase(StrRightBack(fileName,"."))="xlsm")Then
		FileTypeXl=True
	Else
		FileTypeXl=False
	End If
	
	Exit Function
errHandler:
	Print "Error in function FileTypeXl in an agent agentProjectRequest "+Error+" at line number: "+CStr(Erl)+"Error code: "+CStr(Err)
	Exit Function 
End Function

