Function updateEditHistory(doc As NotesDocument, activity As String,curApprove As String) 
	'******************************************************************************************
	'@Author:	Karthikeyan A
	'@Created:	06-Sep-2010
	'@Type:		Function
	'@Purpose:	To perform the audit Trial functionality
	'@Name:		upDateAudit
	'@Param:	activity As String - the information that you want to update the audit trial with
	'@Param:	isWebTriggered As boolean - true : indicates the function is triggered from web
	'			and false: indicates other wise 
	'@Update History:
	'******************************************************************************************

	'declare all variables and objects necessary for further manipulation
	Dim session As New NotesSession
	Dim sno As String
	Dim userNames As String
	Dim dates As String
	Dim activities As String
	Dim totalElements As Integer
	Dim snoArray As Variant
	Dim currUserName As NotesName
	Dim currUser As String
	
	
	'handle errors in case of abrupt termination
	On Error GoTo errHandler
	
	
	Call logUpdate("log initialized")
	
	'get the handle for the current user name's object
	prevApprover=curApprove
	Set currUserName=session.CreateName(prevApprover)
	'Set currUserName=session.CreateName(prevApprover)'UserName)
	
	'if the current document is a new document the updaet audit and exit
	If doc.Isnewnote Then
		Set currUserName=session.CreateName(doc.cSalesEx(0))
		Call doc.Replaceitemvalue(AUDIT_SNO_FIELD,"1")
		Call doc.Replaceitemvalue(AUDIT_USERNAME_FIELD,currUserName.Abbreviated)
		Call doc.Replaceitemvalue(AUDIT_DATE_FIELD,CStr(Now))
		Call doc.Replaceitemvalue(AUDIT_ACTIVITY_FIELD,activity)		
	Else
		'if the document is not a new one then get the existing values and prepend the 
		'activity on top of them
		snoArray=doc.GetItemValue(AUDIT_SNO_FIELD)
		totalElements=UBound(snoArray)+1
		sno= Join(doc.getItemValue(AUDIT_SNO_FIELD),";")
		userNames= Join(doc.getItemValue(AUDIT_USERNAME_FIELD),";")
		dates= Join(doc.getItemValue(AUDIT_DATE_FIELD),";")
		activities= Join(doc.getItemValue(AUDIT_ACTIVITY_FIELD),";")
		
		sno=sno &amp; ";" &amp; CStr(totalElements+1)
		userNames=currUserName.Abbreviated &amp; ";" &amp; userNames 
		dates= CStr(Now) &amp; ";" &amp; dates
		activities= activity &amp; ";" &amp; activities
		
		Call doc.Replaceitemvalue(AUDIT_SNO_FIELD,sno)
		Call doc.Replaceitemvalue(AUDIT_USERNAME_FIELD,userNames)
		Call doc.Replaceitemvalue(AUDIT_DATE_FIELD,dates)
		Call doc.Replaceitemvalue(AUDIT_ACTIVITY_FIELD,activities)
	End If
	'Call doc.Save(True,False) 
	Exit Function
errHandler:
	'print "Error : " &amp; Error &amp; " occured on line :" &amp; CStr(Err) &amp; " with number :" &amp; CStr(Erl) &amp; " in function updateEditHistory(doc As NotesDocument, activity As String) "
	Exit Function
End Function

