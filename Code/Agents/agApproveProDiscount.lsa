	@Purppose : Regarding discount approval process. 
%END REM
Function Approve(curDoc As NotesDocument)
	Dim session As New NotesSession
	Dim docReaderItem As NotesItem		
	On Error GoTo ErrHandler
	'Getting current database handle....
	Set curDb=session.Currentdatabase
	'Getting SE Name from document.... This SE Name act As a key For taking area configuration document....
	seName=curDoc.Getitemvalue(FIELD_SALES_EXE)(0)
	asmname=curDoc.getitemvalue(FIELD_ASM)(0)
	
	key=seName

	'Getting the area configuration document
	Set areaConfigDoc=getLookupDoc(key)
	If areaConfigDoc Is Nothing Then
		MsgBox "Error in taking hadle of area configuration document....!"
		Exit Function 
	End If
	'Getting RSM,MKTG,NSM,VP,MD,LE and RLM name from area configuration document.
	rsmName=getRSMName(areaConfigDoc)	
	mktgName=getMKTGName(areaConfigDoc)
	nsmName=getNSMName(areaConfigDoc)
	vpName=getVPName(areaConfigDoc)
	mdName=getMDName(areaConfigDoc)
	leName=getLEName(areaConfigDoc)
	rlmName = getRLMName(areaConfigDoc)
	ramName = getRamName(areaConfigDoc)
	
	
	'Getting final approver value from document.
	finalrouter=curDoc.Getitemvalue(FIELD_ROUTING_LEVEL)(0)
	'Getting current approver value from document.
	curSubmitStage=curDoc.Getitemvalue(FIELD_SubmitStage)(0)
	'Call doApprove(curDoc,currentstage)
	
	currentstage=curDoc.Getitemvalue(FIELD_STATUS)(0)

	If(curSubmitStage="SESubmit") Then
		Call submitDocument(curDoc)
	ElseIf(curSubmitStage="SEResubmit") Then
		Call SeresubmitDocument(curDoc)
	ElseIf(curSubmitStage="ASMSubmit") Then
		Call asmApproval(curDoc)
	ElseIf(curSubmitStage="ASMRework") Then
		Call asmRework(curDoc)		
	ElseIf(curSubmitStage="RSMSubmit") Then
		Call rsmApproval(curDoc)
	ElseIf(curSubmitStage="RSMRework") Then
		Call rsmRework(curDoc)
	ElseIf(curSubmitStage="MKTGSubmit") Then
		Call mktgUpload(curDoc)
	ElseIf(curSubmitStage="MKTGRework") Then
		Call mktgRework(curDoc)
	ElseIf(curSubmitStage="NSMSubmit") Then
		Call nsmApproval(curDoc)
	ElseIf(curSubmitStage="NSMRework") Then
		Call nsmRework(curDoc)
	ElseIf(curSubmitStage="VPSubmit") Then
		Call vpApproval(curDoc)
	ElseIf(curSubmitStage="VPRework") Then
		Call vpRework(curDoc)
	ElseIf(curSubmitStage="MDSubmit") Then
		Call mdApproval(curDoc)
	ElseIf(curSubmitStage="MDRework") Then
		Call mdRework(curDoc)
	ElseIf(curSubmitStage="LESubmit") Then
		Call completeOrder(curDoc)
	End If	
	Call updateReader(docReaders, curDoc)
	Call updateAuthor(docAuthors, curDoc)
	Exit Function
ErrHandler:
	MsgBox "Error in Approve function: "+Error+ ": at line number: "+CStr(Erl)
	Exit Function
End Function

