%REM
	@Created  : June-20-2011
	@Author   : Gnanasuriyan A
	@Purpose  : VP Approval process.....
%END REM 
Function vpApproval(curDoc As NotesDocument)
	On Error GoTo errHandler
	Call updateEditHistory(curDoc, "Approved by VP",vpname)
	Call curDoc.Replaceitemvalue("vpApDate",CDat(Now()))
	If(Trim(finalrouter)=Trim(currentstage)) Then		
		Call leStage(curDoc,vpname)
		Call updateReaderString(getCanonicalName(asmName),curDoc)
		Call updateReaderString(getCanonicalName(rsmName),curDoc)
		Call updateReaderString(getCanonicalName(mktgName),curDoc)
		Call updateReaderString(getCanonicalName(nsmName),curDoc)
		Call updateReaderString(getCanonicalName(vpName),curDoc)
		Call updateReaderString(getCanonicalName(seName),curDoc)
		
		Call sendMail(curDoc,"",asmname,vpname)
		Call sendMail(curDoc,"",rsmname,vpname)
		Call sendMail(curDoc,"",mktgname,vpname)
		Call sendMail(curDoc,"",nsmname,vpname)
		Call sendMail(curDoc,"",vpname,vpname)
		Call sendMail(curDoc,"",sename,vpname)	
	Else
		'Set the reader as VP  
		Call updateReaderString(getCanonicalName(mdName),curDoc)
		Call updateReaderString(getCanonicalName(vpName),curDoc)
		Call curDoc.Replaceitemvalue(FIELD_CURRENT_STAGE,"6")
		Call curDoc.Replaceitemvalue(FIELD_STATUS_MSG,"Awaiting Approval-MD")
		'Set the author as MD
		docAuthors=mdName
		Call curDoc.Replaceitemvalue(FIELD_NEXT_APPROVER, mdName)
		Call sendMail(curDoc,mdName,"",vpname)
	End If
	Exit Function
errHandler:
	MsgBox "Error in vpApproval function: "+Error+ ": at line number: "+CStr(Erl)
	Exit Function
End Function

