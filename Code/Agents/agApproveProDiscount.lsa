	Const FIELD_NSM_PROPOSED="NSMProposed"
	Const FIELD_VP_PROPOSED="VPProposed"
	Const FIELD_MD_PROPOSED="MDProposed"
	Const FIELD_YEAR="txtYear"
	Const FIELD_CONFIG_PN="txtConfigPN"
	Const FIELD_PROPOSAL_NUMBER="txtproposalno"
	Dim FIELD_DISCOUNT_PWS As String
	Dim FIELD_DISCOUNT_JPS As String
	Dim FIELD_DISCOUNT_APS As String
	Dim FIELD_DISCOUNT_TAPS As String
	Dim FIELD_DISCOUNT_CGR As String
	Dim FIELD_DISCOUNT_RST As String
	Dim FIELD_DISCOUNT_RTS As String
	
	Dim FIELD_DISCOUNT_PWS_NEW As String
	Dim FIELD_DISCOUNT_JPS_NEW As String
	Dim FIELD_DISCOUNT_APS_NEW As String
	Dim FIELD_DISCOUNT_TAPS_NEW As String
	Dim FIELD_DISCOUNT_CGR_NEW As String
	Dim FIELD_DISCOUNT_RST_NEW As String
	Dim FIELD_DISCOUNT_RTS_NEW As String
	Dim FIELD_DISCOUNT_OWN_NEW As String
	Dim FIELD_DISCOUNT_AFSM_NEW As String
	Dim FIELD_DISCOUNT_RCC_NEW As String
	Dim FIELD_DISCOUNT_L_NEW As String
	
	If(isRecalculated) Then
		FIELD_DISCOUNT_PWS="nmadpws"
		FIELD_DISCOUNT_JPS="nmadjps"
		FIELD_DISCOUNT_APS="nmadaps"
		FIELD_DISCOUNT_TAPS="nmadtaps"
		FIELD_DISCOUNT_CGR="nmadcgr"
		FIELD_DISCOUNT_RST="nmadrst"
		FIELD_DISCOUNT_RTS="nmadrts"
		
		FIELD_DISCOUNT_PWS_NEW="nmadpws_new"
		FIELD_DISCOUNT_JPS_NEW="nmadjps_new"
		FIELD_DISCOUNT_APS_NEW="nmadaps_new"
		FIELD_DISCOUNT_TAPS_NEW="nmadtaps_new"
		FIELD_DISCOUNT_CGR_NEW="nmadcgr_new"
		FIELD_DISCOUNT_RST_NEW="nmadrst_new"
		FIELD_DISCOUNT_RTS_NEW="nmadrts_new"
		FIELD_DISCOUNT_OWN_NEW="nmadown_new"
		FIELD_DISCOUNT_AFSM_NEW="nmadafsm_new"
		FIELD_DISCOUNT_RCC_NEW="nmadrcc_new"
		FIELD_DISCOUNT_L_NEW="nmadl_new"

		Call reqdoc.Replaceitemvalue(FIELD_RECALCULATION, "Yes")
	Else
		
		FIELD_DISCOUNT_PWS="nmdpws"
		FIELD_DISCOUNT_JPS="nmdjps"
		FIELD_DISCOUNT_APS="nmdaps"
		FIELD_DISCOUNT_TAPS="nmdtaps"
		FIELD_DISCOUNT_CGR="nmdcgr"
		FIELD_DISCOUNT_RST="nmdrst"
		FIELD_DISCOUNT_RTS="nmdrts"
		
		FIELD_DISCOUNT_PWS_NEW="nmdpws_new"
		FIELD_DISCOUNT_JPS_NEW="nmdjps_new"
		FIELD_DISCOUNT_APS_NEW="nmdaps_new"
		FIELD_DISCOUNT_TAPS_NEW="nmdtaps_new"
		FIELD_DISCOUNT_CGR_NEW="nmdcgr_new"
		FIELD_DISCOUNT_RST_NEW="nmdrst_new"
		FIELD_DISCOUNT_RTS_NEW="nmdrts_new"
		FIELD_DISCOUNT_OWN_NEW="nmdown_new"
		FIELD_DISCOUNT_AFSM_NEW="nmdafsm_new"
		FIELD_DISCOUNT_RCC_NEW="nmdrcc_new"
		FIELD_DISCOUNT_L_NEW="nmdl_new"
		
	End If
	
	On Error GoTo errHandler
	
	'Check the request document status
	If reqdoc Is Nothing Then
		'print "Error in document handle"
		Exit Function
	End If
	
	If Not isRecalculated Then
		'Proposal Number Generation
		Set configPNView=curDb.Getview("vwConfigProposalNumber")
		'Check config view handle
		If configPNView Is Nothing Then
			'Msgbox "Configuration view handle is not got"
			Exit Function
		End If
		'Get the proposal number configuration document
		Set configPNDoc=configPNView.Getfirstdocument()
		'Check the proposal number configuration document status
		If configPNDoc Is Nothing Then
			'Msgbox "Proposal number configuration document is not got"
			Exit Function
		End If
		'Get the year from configuration document
		docYear=configPNDoc.Getitemvalue(FIELD_YEAR)(0)
		'Get the document proposal number
		docPN=configPNDoc.Getitemvalue(FIELD_CONFIG_PN)(0)
		'Increment docPN value and saved 
		If reqdoc.txtProposalNo(0)="" Then
			configPNDoc.txtConfigPN=CStr(CInt(docPN)+1)
			Call configPNDoc.Save(True,False)
			docPN=CStr(CInt(docPN)+1)
			
			If(Len(docPN)=1) Then
				Call reqdoc.Replaceitemvalue(FIELD_PROPOSAL_NUMBER,docYear+"-00000"+docPN)
			ElseIf (Len(docPN)=2) Then
