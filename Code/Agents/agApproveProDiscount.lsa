	@Purpose  : This function just save the document and sent mail to ASM. 
				Now currentStage = "Awaiting approval ASM"
%END REM
Function submitDocument(curDoc As NotesDocument) 
	On Error GoTo errHandler
	
	'Getting ASM name from document.
	asmName=curDoc.Getitemvalue(FIELD_ASM)(0)	
	requestor=curDoc.Getitemvalue("Creator")(0)
	Call curDoc.Replaceitemvalue("seApDate",CDat(Now()))			
	Call curDoc.Replaceitemvalue(FIELD_CURRENT_STAGE,"2")
	Call curDoc.Replaceitemvalue(FIELD_STATUS_MSG,"Awaiting Approval-ASM")	
	Call updateEditHistory(curDoc, "Document created By SE",seName)
	Call updateReaderString(getCanonicalName(seName),curDoc)
	Call updateReaderString(getCanonicalName(asmName),curDoc)
	Call updateReaderString(getCanonicalName(ramname),curDoc)
	docAuthors=asmName
	
	Call curDoc.Replaceitemvalue(FIELD_NEXT_APPROVER, asmName)
	CurDoc.Form="NewProjectDiscount"
	Call CurDoc.save(True,False)
	Call sendMail(curDoc,asmName,"",seName)
	Call sendMail(curDoc,"",ramName,seName)	
	
	Exit Function
errHandler:
	MsgBox "Error in submitDocument function: "+Error+ ": at line number: "+CStr(Erl)
	Exit Function
End Function

